{
    "name": "jnjxp/molniya",
    "description": "Simple middleware and helper for flash messages",
    "homepage": "https://github.com/jnjxp/jnjxp.molniya",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1|^8.0",
        "psr/http-message": "^1.0",
        "mezzio/mezzio-flash": "^1.0",
        "mezzio/mezzio-template": "^2.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^8.2",
        "laminas/laminas-diactoros": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Jnjxp\\Molniya\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jnjxp\\Molniya\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "config-provider": [
                "Jnjxp\\Molniya\\ConfigProvider"
            ]
        }
    }
}
